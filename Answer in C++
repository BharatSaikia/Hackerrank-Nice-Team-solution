#include <iostream>
using namespace std;

int main()
{
    int array[] = {1,5,2,3,6,9};
    int count = 0;
    int min_diff = 2;
    int size = sizeof(array)/sizeof(array[0]);
    for(int i=0; i<size-1; i++){
        for(int j=i+1; j<size ; j++){
            
            //cout<<array[i]<<" - "<<array[j]<<" = "<<array[i]-array[j]<<endl;
            
            if(abs(array[i] - array[j]) == min_diff)
                count++;
        }

    }
    cout<<c<<endl;
    return 0;
}
